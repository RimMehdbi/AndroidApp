<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rim.asus.gestioncontact">
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Authentification">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Accueil" />
        <activity
            android:name=".Ajout"
            android:label="@string/title_activity_ajout"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Afficher"
            android:label="@string/title_activity_afficher"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>